OsrMonster (Monster) {
  Title! :: STRING
  ArmourClass! :: STRING
  HitDiceText :: STRING
  ExHitDice! :: STRING
  HitDiceRoll! :: STRING
  HitDice! :: INTEGER
  Attacks! :: STRING
  THAC0! :: STRING
  Movement! :: STRING
  SavingThrows! :: STRING
  Morale! :: STRING
  Alignment! :: STRING
  XP! :: STRING
  NumberAppearingRoaming! :: DICE
  NumberAppearingLair! :: DICE
  TreasureType! :: TYPE(TreasureType)
  Stats! ~ <%
    <span class='monsterstats'>
    <strong>Armour Class:</strong> {{ArmourClass}} <strong>Hit Dice:</strong> {{ExHitDice}} <strong>Attacks:</strong> {{Attacks}}
    <strong>THAC0:</strong> {{THAC0}} <strong>Movement:</strong> {{Movement}} <strong>Saving Throws:</strong> {{SavingThrows}}
    <strong>Morale:</strong> {{Morale}} <strong>Alignment:</strong> {{Alignment}} <strong>XP:</strong> {{XP}}
    </span>
  %>
  | Monster
}
Acolyte (OsrMonster) {
  Title! ~ "Acolyte"
  Intelligent! = True
  ArmourClass! ~ "2 [17]"
  HitDiceText ~ "1 (4hp)"
  ExHitDice! = 1
  HitDiceRoll! = 1d8
  HitDice! = 1
  Attacks! ~ "1 × mace (1d6)"
  THAC0! ~ "19 [0]"
  Movement! ~ "60’ (20’)"
  SavingThrows! ~ "D11 W12 P14 B16 S15 (C1)"
  Morale! = "7"
  Alignment! ~ "Any"
  XP! = "10"
  NumberAppearingRoaming! @ 1d8
  NumberAppearingLair! @ 1d20
  TreasureType! @ TreasureTypeU {
        MaxNumberAppearingRoaming = 8
        NumberAppearingRoaming = &NumberAppearingRoaming
        NumberAppearingLair = &NumberAppearingLair
    }
  Activity! @ MinionActivity {
      NumberAppearingRoaming = &NumberAppearingRoaming
      NumberAppearingLair = &NumberAppearingLair
  }
  | OsrMonster
}

